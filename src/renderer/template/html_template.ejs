        /
        /
        -->
    </style>
</head>
<body bgcolor='#ffffff' topmargin="0">
<table width="100%" border="0" cellspacing="0" cellpadding="5">
    <tr>
        <td class="toptext"><p align="center">Schema for '<%= data.databaseName %>'</td>
    </tr>
</table>
<a name="header">&nbsp</a>
<ul>
    <% data.tableList.forEach(table=>{ %>
        <li><a href='#<%= table.tableName %>'><p class="normal"><%= table.tableName %>
                    <% if (table.tableComment) { %>
                        （ <%= table.tableComment %>）
                    <% } %>
            </a></li>
    <% }) %>
</ul>
<% data.tableList.forEach( table =>{ %>
    <br class=page>
    <p><a name='<%= table.tableName %>'>&nbsp</a>
    <table width="100%" border="0" cellspacing="0" cellpadding="3">
        <tr>
            <td class="headtext" width="30%" align="left" valign="top"><%= table.tableName %></td>
            <td>&nbsp</td>
        <tr>
    </table>
    <table width="100%" cellspacing="0" cellapdding="2" border="1">
        <tr>
            <td align="center" valign="top" class="fieldcolumn">Field</td>
            <td align="center" valign="top" class="fieldcolumn">Type</td>
            <td align="center" valign="top" class="fieldcolumn">Collation</td>
            <td align="center" valign="top" class="fieldcolumn">Charset</td>
            <td align="center" valign="top" class="fieldcolumn">Null</td>
            <td align="center" valign="top" class="fieldcolumn">Key</td>
            <td align="center" valign="top" class="fieldcolumn">Default</td>
            <td align="center" valign="top" class="fieldcolumn">Comment</td>
        </tr>
        <% table.columns.forEach( column=>{ %>
            <tr>
                <td align="left" valign="top"><p class="normal"><%= column.columnName %></td>
                <td align="left" valign="top"><p class="normal"><%= column.columnType %></td>
                <td align="left" valign="top"><p class="normal"><%= column.collationName %></td>
                <td align="left" valign="top"><p class="normal"><%= column.characterSetName %></td>
                <td align="left" valign="top"><p class="normal"><%= column.isNullable %></td>
                <td align="left" valign="top"><p class="normal"><%= column.columnKey %></td>
                <td align="left" valign="top"><p class="normal"><%= column.columnDefault %></td>
                <td align="left" valign="top"><p class="normal"><%= column.columnComment %></td>
            </tr>
        <% }) %>
    </table>
    <a href="#header"><p class="normal">Back</a><br class=page>
<% }) %>
